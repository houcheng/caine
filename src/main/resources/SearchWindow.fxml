<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="SearchWindow" alignment="center" prefWidth="731.0" xmlns="http://javafx.com/javafx/8.0.102-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.caine.ui.SearchController">
   <children>
      <TextField fx:id="inputTextField" onKeyPressed="#handleKeyPressed" onKeyTyped="#handleKeyTypedOnTextField" promptText="search keyword">
         <font>
            <Font size="32.0" />
         </font>
      </TextField>
      <ListView fx:id="listView" onKeyPressed="#handleKeyPressed" onMouseClicked="#handleMouseClickedOnList">
      </ListView>
   </children>
</VBox>
